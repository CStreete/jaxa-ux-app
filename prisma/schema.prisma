generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Comment {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model Recepie {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model User {
  id             String @id @default(auto()) @map("_id") @db.ObjectId
  email          String @unique
  hashedPassword String
  name           String
  username       String @unique
}
